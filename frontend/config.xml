<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<widget id="org.incode.eurocommercialproperties.contactapp" version="1.0" android-versionCode="4"
        xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
  <name>ECP</name>
  <description>&#xD;
    EuroCommercial's ContactApp&#xD;
  </description>
  <author email="jvanderwal@incode.org" href="http://incode.org/">&#xD;
    incode.org&#xD;
  </author>
  <content src="index.html" />
  <access origin="*" />
  <access origin="tel:*" launch-external="yes" />
  <access origin="mailto:*" launch-external="yes" />

  <allow-intent href="tel:*" />
  <allow-intent href="mailto:*" />

  <!-- Test (see preferences.js) -->
  <allow-navigation href="http://10.0.0.5:8080"       />
  <!-- Production (see preferences.js) -->
  <allow-navigation href="http://contacts.ecpnv.com"  />
  <allow-navigation href="https://contacts.ecpnv.com" />

  <preference name="webviewbounce"         value="false"    />
  <preference name="UIWebViewBounce"       value="false"    />
  <preference name="DisallowOverscroll"    value="true"     />
  <preference name="android-minSdkVersion" value="16"       />
  <preference name="BackupWebStorage"      value="none"     />
  <preference name="SplashScreen"          value="screen"   />
  <preference name="SplashScreenDelay"     value="3000"     />
  <preference name="Orientation"           value="portrait" />
  <feature name="StatusBar">
    <param name="ios-package" value="CDVStatusBar" onload="true"/>
  </feature>
  <platform name="ios">
    <preference name="KeyboardDisplayRequiresUserAction" value="false" />
    <icon src="resources/ios/icon/icon-24@2x.png"       width="48"  height="48"  />
    <icon src="resources/ios/icon/icon-27.5@2x.png"     width="55"  height="55"  />
    <icon src="resources/ios/icon/icon-29@2x.png"       width="58"  height="58"  />
    <icon src="resources/ios/icon/icon-29@3x.png"       width="87"  height="87"  />
    <icon src="resources/ios/icon/icon-40.png"          width="40"  height="40"  />
    <icon src="resources/ios/icon/icon-40@2x.png"       width="80"  height="80"  />
    <icon src="resources/ios/icon/icon-40@3x.png"       width="120" height="120" />
    <icon src="resources/ios/icon/icon-44@2x.png"       width="88"  height="88"  />
    <icon src="resources/ios/icon/icon-57.png"          width="57"  height="57"  />
    <icon src="resources/ios/icon/icon-57@2x.png"       width="114" height="114" />
    <!-- <icon src="resources/ios/icon/icon-60.png"          width="60"  height="60"  /> -->
    <icon src="resources/ios/icon/icon-60@2x.png"       width="120" height="120" />
    <icon src="resources/ios/icon/icon-60@3x.png"       width="180" height="180" />
    <icon src="resources/ios/icon/icon-72.png"          width="72"  height="72"  />
    <icon src="resources/ios/icon/icon-72@2x.png"       width="144" height="144" />
    <icon src="resources/ios/icon/icon-76.png"          width="76"  height="76"  />
    <icon src="resources/ios/icon/icon-76@2x.png"       width="152" height="152" />
    <icon src="resources/ios/icon/icon-83.5@2x.png"     width="167" height="167" />
    <icon src="resources/ios/icon/icon-86@2x.png"       width="172" height="172" />
    <icon src="resources/ios/icon/icon-98@2x.png"       width="196" height="196" />
    <icon src="resources/ios/icon/icon-small-29.png"    width="29"  height="29"  />
    <icon src="resources/ios/icon/icon-small-29@2x.png" width="58"  height="58"  />
    <icon src="resources/ios/icon/icon-small-29@3x.png" width="87"  height="87"  />
    <icon src="resources/ios/icon/icon-small-50.png"    width="50"  height="50"  />
    <icon src="resources/ios/icon/icon-small-50@2x.png" width="100" height="100" />
  </platform>
  <platform name="android">
    <icon src="resources/android/icon/ldpi.png"    density="ldpi"    />
    <icon src="resources/android/icon/mdpi.png"    density="mdpi"    />
    <icon src="resources/android/icon/hdpi.png"    density="hdpi"    />
    <icon src="resources/android/icon/xhdpi.png"   density="xhdpi"   />
    <icon src="resources/android/icon/xxhdpi.png"  density="xxhdpi"  />
    <icon src="resources/android/icon/xxxhdpi.png" density="xxxhdpi" />
    <splash src="resources/android/splash/land-ldpi.png"    density="land-ldpi"    />
    <splash src="resources/android/splash/land-mdpi.png"    density="land-mdpi"    />
    <splash src="resources/android/splash/land-hdpi.png"    density="land-hdpi"    />
    <splash src="resources/android/splash/land-xhdpi.png"   density="land-xhdpi"   />
    <splash src="resources/android/splash/land-xxhdpi.png"  density="land-xxhdpi"  />
    <splash src="resources/android/splash/land-xxxhdpi.png" density="land-xxxhdpi" />
    <splash src="resources/android/splash/port-ldpi.png"    density="port-ldpi"    />
    <splash src="resources/android/splash/port-mdpi.png"    density="port-mdpi"    />
    <splash src="resources/android/splash/port-hdpi.png"    density="port-hdpi"    />
    <splash src="resources/android/splash/port-xhdpi.png"   density="port-xhdpi"   />
    <splash src="resources/android/splash/port-xxhdpi.png"  density="port-xxhdpi"  />
    <splash src="resources/android/splash/port-xxxhdpi.png" density="port-xxxhdpi" />
  </platform>
</widget>
